{% block body %}
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<div class="container" style="background-color: #f2f2f2;border: 2px solid black;margin-top: 2%">
    Vous êtes dans la liste des formation: </br></br>
    {% if (message) %}
        {{ message }}
    {% endif %}
    {% for inscription in ensInscription %}
        {% if (inscription.statut != 'E') %}
            {% for formation in ensFormation %}
                Date de début: <b>{{ formation.dateDebut|date("m/d/y") }}</b><br>
                nombre heure : {{ formation.nbreHeaures }}</br>
                departement : {{ formation.departement }}</br>
                resume : {{ formation.resume }}</br>
                <a href="{{path('app_inscription' , {'id': formation.id})}}">s'inscrire</a></br></br>
             {% endfor %}
       {% endif %}
    {% endfor %}
</div>

{% endblock %}